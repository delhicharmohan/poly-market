{
  "name": "poly-market",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:up": "docker-compose up -d postgres",
    "db:down": "docker-compose stop postgres",
    "db:logs": "docker-compose logs -f postgres",
    "db:shell": "docker-compose exec postgres psql -U indimarket -d indimarket",
    "db:init": "node scripts/db-init.js",
    "db:backup": "docker-compose exec postgres pg_dump -U indimarket indimarket > backup-$(date +%Y%m%d-%H%M%S).sql",
    "db:reset": "docker-compose down -v && docker-compose up -d postgres",
    "db:migrate-sales": "node scripts/run-add-sales-table.js"
  },
  "dependencies": {
    "@types/pg": "^8.16.0",
    "axios": "^1.7.7",
    "canvas-confetti": "^1.9.4",
    "firebase": "^12.7.0",
    "lucide-react": "^0.424.0",
    "next": "14.2.5",
    "pdf-lib": "^1.17.1",
    "pg": "^8.16.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^20.14.12",
    "@types/pg": "^8.10.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.40",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4"
  }
}
